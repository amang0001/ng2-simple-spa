{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"npm run json-server\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "json-server": "json-server --delay 500 --watch json-server/db.json --routes json-server/routes.json --port 9999"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "@angular2-material/button": "^2.0.0-alpha.5",
    "@angular2-material/card": "^2.0.0-alpha.5",
    "@angular2-material/core": "^2.0.0-alpha.5",
    "@angular2-material/grid-list": "^2.0.0-alpha.5",
    "@angular2-material/icon": "^2.0.0-alpha.5",
    "@angular2-material/input": "^2.0.0-alpha.5",
    "@angular2-material/list": "^2.0.0-alpha.5",
    "@angular2-material/progress-circle": "^2.0.0-alpha.5",
    "@angular2-material/toolbar": "^2.0.0-alpha.5",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "ng2-translate": "^2.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "codelyzer": "0.0.19",
    "concurrently": "^2.0.0",
    "connect-history-api-fallback": "^1.2.0",
    "http-proxy-middleware": "^0.15.2",
    "json-server": "^0.8.14",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
